<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include ../templates/header.ejs %>
  </head>
  <body class="">
    <div id="app">
      <div class="main-wrapper">
        <%- include ../templates/navtop.ejs %> <%- include
        ../templates/navleft_user.ejs %>
        <div class="main-content" style="padding-top: 90px">
          <section class="section">
            <div class="section-header">
              <h1>TOYOTA HOW</h1>
              <div class="section-header-breadcrumb">
                <div class="breadcrumb-item active">
                  <a href="#">Dashboard</a>
                </div>
                <div class="breadcrumb-item">ToyotaHow</div>
              </div>
              <% if(alert.message != '') { %>
              <br />
              <div
                class="alert alert-<%= alert.status %>"
                style="text-align: center"
              >
                <strong><%= alert.message %></strong>
              </div>
              <% } %>
            </div>

            <div class="section-body">
              <form action="">
                <div class="form-group">
                  <div class="input-group">
                    <input
                      type="text"
                      class="form-control"
                      placeholder="Search Yuor Problem..."
                    />
                    <div class="input-group-append">
                      <div class="input-group-text">
                        <i class="fas fa-search"></i>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
              <div class="row">
                <% for (var i = 0; i < data.length; i++) { %>
                <div class="col-12 col-sm-6 col-md-6 col-lg-3">
                  <article class="article">
                    <div class="article-header">
                      <div
                        class="article-image"
                        data-background="/assets/toyotahow/sampul/sampul.jpg"
                      ></div>
                      <div class="article-title">
                        <h2>
                          <a href="/toyotahow/detail"><%= data[i].judul %></a>
                        </h2>
                      </div>
                    </div>
                    <div class="article-details">
                      <div class="article-cta">
                        <a href="/toyotahow/detail" class="btn btn-primary"
                          >Yuk Pelajari</a
                        >
                      </div>
                    </div>
                  </article>
                </div>
                <% } %>
                <div class="col-12 col-sm-6 col-md-6 col-lg-3">
                  <article class="article">
                    <div class="article-header">
                      <div
                        class="article-image"
                        data-background="/assets/toyotahow/sampul/sampul2.jpg"
                      ></div>
                      <div class="article-title">
                        <h2>
                          <a href="#">Cara Memoles Mobil</a>
                        </h2>
                      </div>
                    </div>
                    <div class="article-details">
                      <div class="article-cta">
                        <a href="#" class="btn btn-primary">Yuk Pelajari</a>
                      </div>
                    </div>
                  </article>
                </div>
                <div class="col-12 col-sm-6 col-md-6 col-lg-3">
                  <article class="article">
                    <div class="article-header">
                      <div
                        class="article-image"
                        data-background="/assets/toyotahow/sampul/sampul3.jpg"
                      ></div>
                      <div class="article-title">
                        <h2>
                          <a href="#">Cara Berciuman di dalam Mobil</a>
                        </h2>
                      </div>
                    </div>
                    <div class="article-details">
                      <div class="article-cta">
                        <a href="#" class="btn btn-primary">Yuk Pelajari</a>
                      </div>
                    </div>
                  </article>
                </div>
                <div class="col-12 col-sm-6 col-md-6 col-lg-3">
                  <article class="article">
                    <div class="article-header">
                      <div
                        class="article-image"
                        data-background="/assets/toyotahow/sampul/sampul4.jpg"
                      ></div>
                      <div class="article-title">
                        <h2>
                          <a href="#">Cara Menyalakan Lampu Depan Mobil</a>
                        </h2>
                      </div>
                    </div>
                    <div class="article-details">
                      <div class="article-cta">
                        <a href="#" class="btn btn-primary">Yuk Pelajari</a>
                      </div>
                    </div>
                  </article>
                </div>
              </div>
            </div>
          </section>
        </div>
        <%- include ../templates/footer.ejs %>
      </div>
    </div>
    <%- include ../templates/script.ejs %>
  </body>
</html>
