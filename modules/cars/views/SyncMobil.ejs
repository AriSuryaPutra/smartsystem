      let url = '<%= process.env.URL %>/cars/ceknorangka/' + idData;
      console.log(url);
      $.ajax({
        type: 'POST',
        url: url,
        dataType: 'json',
        success: function (res) {
          if (res) {
            
          }
        }
      });
    };
</script>